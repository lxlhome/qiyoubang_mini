.list{padding-top: 26rpx;}
.list .item{
  margin-bottom: 26rpx;
  width: 690rpx;
	height: 240rpx;
	background-color: #ffffff;
	box-shadow: 0px 1px 2px 0px 
		rgba(96, 97, 112, 0.16);
  border-radius: 5px;
  padding: 20rpx;
  display: flex;
  justify-content: space-between;
}
.list .item image{
  width: 200rpx;
  height: 200rpx;
  border-radius: 5rpx;
}
.list .item .info{
  width: 430rpx;
}
.list .item .info .title{
  font-weight: bold;
  height: 50rpx;
}
.list .item .info .rank{
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
  height: 50rpx;
}
.list .item .info .tel{
  font-size: 24rpx;
  height: 50rpx;
}
.list .item .info .address{
  font-size: 24rpx;
  height: 50rpx;
}